{
  "ConnectionStrings": {
    "AppConnString": "{DB_connection}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Jwt": {
    "FireBase": {
      "ValidIssuer": "{firebase_project}",
      "ValidAudience": "firebase_project_app"
    }
  },
  "AllowedHosts": "*"
}
